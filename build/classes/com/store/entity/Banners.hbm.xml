<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.store.entity">
   
     <class name="Banners" table="Banners">
        <id name="BannerId" column="BannerId" type="int">
            <generator class="identity"/>
        </id>
      <property name="BannerOne" column="BannerOne" type="string"/>
      <property name="link" column="link" type="string"/>
     
    </class>
    
</hibernate-mapping> 